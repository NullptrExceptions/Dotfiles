! Extensions
URxvt.perl-ext-common: default
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! General Appearance
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.font: xft:LiberationMono:size=10:antialiasing=true

! Transparency (change value in square brackets to set transparency, in %)
URxvt.depth: 32
URxvt.background: [60]#000000

! Terminal Colors **
*.foreground:   #ffffff
*.background:   #000000
*.cursorColor:  #ffffff

! black
*.color0:       #252525
*.color8:       #454545

! red
*.color1:       #ef6769
*.color9:       #fc7ca5

! green
*.color2:       #a6e22e
*.color10:      #b6e354

! yellow
*.color3:       #fd971f
*.color11:      #fd971f

! blue
*.color4:       #6495ed
*.color12:      #87ceeb

! magenta
*.color5:       #deb887
*.color13:      #996600

! cyan
*.color6:       #b0c4de
*.color14:      #87ceeb

! white
*.color7:       #dbdcdc
*.color15:      #fdfdfd


! End Terminal Colors **
